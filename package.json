{
    "name": "flipit",
    "displayName": "flipit",
    "description": "Flip active editor contents (or current selection) upside-down",
    "version": "1.0.0",
    "publisher": "ndrake",
    "repository": {
        "type": "git",
        "url": "https://github.com/ndrake/flipit.git"
    },
    "icon": "flipit.png",
    "engines": {
        "vscode": "^1.24.0"
    },
    "categories": [
        "Other"
    ],
    "activationEvents": [
        "onCommand:extension.flipIt"
    ],
    "main": "./extension",
    "contributes": {
        "commands": [
            {
                "command": "extension.flipIt",
                "title": "Flip It"
            }
        ]
    },
    "scripts": {
        "postinstall": "node ./node_modules/vscode/bin/install",
        "test": "node ./node_modules/vscode/bin/test"
    },
    "devDependencies": {
        "typescript": "^2.6.1",
        "vscode": "^1.1.6",
        "eslint": "^4.11.0",
        "@types/node": "^7.0.43",
        "@types/mocha": "^2.2.42"
    },
    "dependencies": {
        "flip": "^1.0.0"
    }
}
