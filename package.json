{
    "name": "flipit",
    "displayName": "flipit",
    "description": "Flip or Zalgolize active editor contents (or current selection) upside-down",
    "version": "1.0.2",
    "publisher": "ndrake",
    "repository": {
        "type": "git",
        "url": "https://github.com/ndrake/flipit.git"
    },
    "icon": "flipit.png",
    "engines": {
        "vscode": "^1.24.0"
    },
    "categories": [
        "Other"
    ],
    "activationEvents": [
        "onCommand:extension.flipIt",
        "onCommand:extension.zalgoIt",
        "onCommand:extension.bubbleIt",
        "onCommand:extension.mirrorIt",
        "onCommand:extension.wonkIt",
        "onCommand:extension.tinyCapsIt"
    ],
    "main": "./extension",
    "contributes": {
        "commands": [
            {
                "command": "extension.flipIt",
                "title": "Flip It"
            },
            {
                "command": "extension.zalgoIt",
                "title": "Zalgo It"
            },
            {
                "command": "extension.bubbleIt",
                "title": "Bubble It"
            },
            {
                "command": "extension.mirrorIt",
                "title": "Mirror It"
            },
            {
                "command": "extension.wonkIt",
                "title": "Wonk It"
            },
            {
                "command": "extension.tinyCapsIt",
                "title": "Tiny Caps It"
            }
        ]
    },
    "scripts": {
        "postinstall": "node ./node_modules/vscode/bin/install",
        "test": "node ./node_modules/vscode/bin/test"
    },
    "devDependencies": {
        "typescript": "3.1.1",
        "vscode": "1.1.21",
        "eslint": "5.6.0",
        "@types/node": "10.11.2",
        "@types/mocha": "5.2.5"
    },
    "dependencies": {
        "flip": "1.0.0",
        "lunicode-bent": "1.0.1",
        "lunicode-bubbles": "1.0.1",
        "lunicode-mirror": "1.0.1",
        "lunicode-tiny": "1.0.1",
        "zalgolize": "1.2.4"
    }
}
